@model RegionViewModel
@using DrcVictim2.Models

@{
    ViewData["Title"] = "Region Page";
}

<div class=" text-center">
    <div class="row">
        <h2 class="display-4"></h2>

        <div class=" col-md-8  offset-2">
            <p class="text-center">Creer la region</p>
            <div class="row">
                <div class=" col-md-8 offset-2">
                    <form asp-action="Region">
                        <div aps-validation-summary="ModelOnly" class="text-danger">
                            <div class="col-md-12 ">

                                <div class=" form-group">
                                    <label asp-for="RegisteredDate" class="control-label"></label>
                                    <input asp-for="RegisteredDate" class="form-control" />
                                    <span asp-validation-for="RegisteredDate" class=" text-danger"></span>

                                </div>
                                <div class=" form-group">
                                    <label asp-for="Name" class="control-label"></label>
                                    <input asp-for="Name" class="form-control" />
                                    <span asp-validation-for="Name" class=" text-danger"></span>

                                </div>


                                <div class=" form-group">

                                    <input type="submit" value=" Create" class="btn btn-outline-success" />


                                </div>

                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

</div>